
create table users(id_no int(16) primary key auto_increment,name text not null,phone text not null,gender enum("m","f","o"),dob date,email varchar(50) not null,password varchar(20) not null)auto_increment=10000;

-----------------------------------------------------------------------------------------------------------------------------------------------------------------

create table bus_list(bus_name text, start text, end text , time text , price int);


mysql> insert into bus_list values("tnstc","tenkasi","chennai","08:00-am",300);


mysql> insert into bus_list values("cheran travels","tenkasi","coimbatore","06:30-am",280);


mysql> insert into bus_list values("kpn travels","chennai","bangalore","10:00-am",200);

